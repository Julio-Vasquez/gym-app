{
  "name": "cool-gym-32",
  "version": "0.1.0",
  "private": true,
  "author": "Julio Alfredo Vasquez Lievano",
  "description": "App create with react and electron",
  "main": "public/electron.js",
  "dependencies": {
    "antd": "^4.6.2",
    "electron-is-dev": "^1.2.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlight-words": "^0.16.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "BROWSER=none && react-scripts start",
    "start-win": "set BROWSER=none && react-scripts start",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev-win": "concurrently \"npm run start-win\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0",
    "wait-on": "^5.2.0"
  },
  "build": {
    "appId": "yourappid",
    "win": {
      "icon": "ico/icon.ico"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
